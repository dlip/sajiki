material Backgrounds/Background3Pipes
{
	technique
	{
		pass
		{
			specular 0.0 0.0 0.0 1.0 12.5
			scene_blend alpha_blend
			texture_unit
			{
				texture backgrounds/background3_pipes.dds
				wave_xform scale_x sine 0.5 0.2 0.5 3
				wave_xform scale_y sine 1 0.1 0 5
				scroll_anim 0.2 0
			}
		}
	}
}

