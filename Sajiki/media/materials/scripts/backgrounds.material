material Backgrounds/Background1
{
	technique
	{
		pass
		{
			scene_blend alpha_blend
			texture_unit
			{
				texture backgrounds/background1.dds
			}
		}
	}
}

material Backgrounds/Background1Goo
{
	technique
	{
		pass
		{
			texture_unit
			{                                          
				texture backgrounds/background1_Goo.dds
				rotate_anim 0.2
			}
			texture_unit
			{
				texture backgrounds/background1_Goo.dds
				colour_op_ex add src_texture src_current
				colour_op_multipass_fallback one one
				scroll_anim -0.1 0.25
				wave_xform scale_x sine 1 1 0 5
				wave_xform scale_y sine 0.5 0.2 0.5 3
			}
		}
	}
}

